{
    "name": "@catsums/replrender",
    "version": "1.0.3",
    "description": "A renderer in typescript used to parse files and strings for html and other files with full customisability",
    "types": "./lib/index.d.ts",
    "main": "./lib/umd/index.js",
    "module": "./lib/index.js",
    "exports": {
        ".": {
            "require": "./lib/cjs/index.js",
            "import": "./lib/esm/index.js"
        }
    },
    "scripts": {
        "bundle": "ts-node build.ts",
        "build": "npm run bundle",
        "update": "ts-node update.ts",
        "npm-publish": "npm publish --access public"
    },
    "keywords": [
        "renderer",
        "express",
        "parser",
        "html",
        "customise",
        "middleware",
        "replacer"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@microsoft/tsdoc": "^0.14.2",
        "esbuild": "^0.19.5",
        "esbuild-plugin-umd-wrapper": "^2.0.0",
        "esmify": "^2.1.1",
        "jsdoc": "^4.0.2",
        "npm-dts": "^1.3.12",
        "ts-node": "^10.9.1"
    },
    "dependencies": {
        "@catsums/my": "^1.2.1",
        "@catsums/vector2": "^1.3.1",
        "lodash": "^4.17.21",
        "svg-path-parser": "^1.1.0"
    }
}